<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhanghao.dao.WorkCardDao">
    <select id="getWorkCardByEmpId" parameterType="int" resultType="com.zhanghao.pojo.WorkCard">
        select * from t_work_card where emp_id=#{empId}
    </select>
</mapper>